
<div class="container my-5">
  <div class="row">
    <div class="col-6 mx-auto">
      <h1>Lists</h1>
      <% @lists.each do |list| %>
        <div>
          <p><%= link_to list.name, list_path(list), class: 'btn btn-primary' %></p>
        </div>
      <% end %>
      <%= link_to 'New list', new_list_path %>
    </div>
  </div>
</div>
